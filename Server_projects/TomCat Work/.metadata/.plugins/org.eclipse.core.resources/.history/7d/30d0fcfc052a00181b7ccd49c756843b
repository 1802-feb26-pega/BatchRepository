package com.proj.services;

import com.proj.pojos.Claim;
import com.proj.pojos.Employee;

public class ClaimServices {

	public Claim addClaim(Claim claim) {
		ClaimDao claim_dao = new ClaimDao();
		Employee employee = new Employee();
		Note note = new Note();
		
		// TODO Auto-generated method stub
		return claim_dao.addUser(claim.getStatus(),claim.getCreated(), claim.getEventStart(),
							claim.getAmount_given(),claim.getLoc(), claim.getEvent_type(), 
							claim.getCost(), claim.getReason(),	claim.getStarttime());
	}

}
